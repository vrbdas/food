{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": 0,
    "no-console": 0,
    "strict": 0,
    "prefer-const": 1,
    "no-unused-vars": 1,
    "prefer-template": 1,
    "no-useless-concat": 1,
    "one-var": 1,
    "no-plusplus": 0,
    "eqeqeq": 0,
    "no-unused-expressions": 1,
    "no-nested-ternary": 1,
    "no-restricted-globals": 1,
    "eol-last": 0,
    "prefer-arrow-callback": 0,
    "dot-notation": 1,
    "no-else-return": 1,
    "no-restricted-syntax": 0,
    "guard-for-in": 0,
    "no-continue": 1,
    "space-before-function-paren": [1, "never"],
    "space-infix-ops": 1,
    "no-use-before-define": ["error", {
      "functions": false,
      "classes": true,
      "variables": false,
      "allowNamedExports": false}],
    "operator-assignment": 1,
    "prefer-object-spread": 1,
    "object-curly-spacing": [1, "never"],
    "space-unary-ops": 1,
    "no-new-object": 1,
    "no-new-wrappers": 1,
    "object-shorthand": 1,
    "quotes": [1, "single", { "allowTemplateLiterals": true }],
    "arrow-parens": [1, "always"],
    "no-param-reassign": [2, { "props": false }],
    "func-names": 0,
    "no-trailing-spaces": 1,
    "no-multiple-empty-lines": [0, { "max": 2, "maxBOF": 0, "maxEOF": 0}],
    "padded-blocks": 0,
    "prefer-destructuring": 1,
    "max-len": [0, { "ignoreComments": true }],
    "consistent-return": 1,
    "no-useless-constructor": 1,
    "max-classes-per-file": 0,
    "lines-between-class-members": 1,
    "comma-dangle": 1,
    "arrow-body-style": 0,
    "prefer-promise-reject-errors": 0,
    "newline-per-chained-call": 1,
    "indent": 1,
    "no-return-await": 0,
    "object-curly-newline": 1
  }
}
